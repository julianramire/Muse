<div class="wrapper" id="aiWrapper">

  <h1>{{ai.title}}</h1>

  <div id="editor">
    {{!-- {{ai.content}} --}}
  </div>
  {{!-- <p>{{ai.content}}</p> --}}

  {{!-- custom helper to see if user id matches the creator's user id --}}
  {{#if (isEqual user.id ai.user_id)}}
  <a href="/editor/{{ai.id}}">
    <p id="edit" class="btn btn-primary">Edit Document</p>
  </a>
  {{/if}}

  <a href="/dashboard">
    <p id="back" class="btn btn-primary">Explore</p>
  </a>

   <a href="/profile">
    <p id="back2" class="btn btn-primary">Profile</p>
  </a>
</div>

<script src="/js/ai.js"></script>
<script src="https://cdn.quilljs.com/1.3.6/quill.js"></script>
<script>
  var quill = new Quill('#editor', {
    theme: 'bubble',
    readOnly: 'true',
  });
</script>